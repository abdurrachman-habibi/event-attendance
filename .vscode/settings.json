// Place your settings in this file to overwrite default and user settings.
{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/bundle/**": true,
    "typings": true
  },
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "node_modules/**": false,
    // "bundle": true,
    "etc": true,
    "npm-debug.log": true
  },
  "files.associations": {
    "*.js": "javascriptreact"
  }
}
